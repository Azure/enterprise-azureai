{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
      "environmentName": {
        "value": "${AZURE_ENV_NAME}"
      },
      "location": {
        "value": "${AZURE_LOCATION}"
      },
      "myIpAddress": {
        "value": "${MY_IP_ADDRESS}"
      },
      "myPrincipalId": {
        "value": "${MY_USER_ID}"
      },
      "useRedisCacheForAPIM" : {
        "value": "${USE_REDIS_CACHE_APIM}"
      },
      "secondaryOpenAILocation" : {
        "value": "${SECONDARY_OPENAI_LOCATION}"
      },
      "apimServiceName": {
        "value": "${APIM_NAME}"
      }
    }
}
